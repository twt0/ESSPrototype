<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="vs.com.essprototype"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">


        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".homepage"
            android:label="@string/title_activity_homepage"
            android:windowSoftInputMode="adjustResize"
            >

        </activity>
        <activity
            android:name=".ListEmployee"
            android:label="@string/title_activity_list_employee" >
        </activity>
        <activity
            android:name=".sEmplyee"
            android:label="@string/title_activity_s_emplyee"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustPan|adjustResize">
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/title_activity_result" >
        </activity>
    </application>

</manifest>
